{
  "name": "@turbo-with-tailwind-v4/supabase",
  "version": "1.0.0",
  "private": true,
  "exports": {
    "./schemas": {
      "import": "./dist/schemas.js",
      "types": "./dist/schemas.d.ts"
    },
    "./types": {
      "import": "./dist/types.js",
      "types": "./dist/types.d.ts"
    },
    "./task-service": {
      "import": "./dist/task-service.js",
      "types": "./dist/task-service.d.ts"
    },
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc --declaration --emitDeclarationOnly false",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^19.1.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.3",
    "@turbo-with-tailwind-v4/design-system": "workspace:*",
    "lucide-react": "^0.475.0",
    "tailwindcss": "^4.1.9",
    "zod": "^3.25.63"
  },
  "devDependencies": {
    "@turbo-with-tailwind-v4/eslint-config": "workspace:*",
    "@turbo-with-tailwind-v4/typescript-config": "workspace:*",
    "@types/react": "^19.0.10",
    "typescript": "^5.8.2"
  }
}