{
  "name": "database",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "exports": {
    "./schemas": {
      "import": "./dist/schemas.js",
      "types": "./dist/schemas.d.ts"
    },
    "./types": {
      "import": "./dist/types.js",
      "types": "./dist/types.d.ts"
    },
    "./task-service": {
      "import": "./dist/task-service.js",
      "types": "./dist/task-service.d.ts"
    },
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc --declaration --emitDeclarationOnly false",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^19.1.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.3",
    "@turbo-with-tailwind-v4/design-system": "workspace:*",
    "lucide-react": "^0.475.0",
    "tailwindcss": "^4.1.9",
    "zod": "^3.25.63"
  },
  "devDependencies": {
    "@turbo-with-tailwind-v4/eslint-config": "workspace:*",
    "@turbo-with-tailwind-v4/typescript-config": "workspace:*",
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}
